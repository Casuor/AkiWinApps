{
    "version": "0.44.5",
    "description": "An AI code editor",
    "homepage": "https://www.cursor.com/",
    "license": "Proprietary",
    "architecture": {
        "64bit": {
            "url": "https://downloader.cursor.sh/windows/nsis/x64",
            "hash": "032cf1ae6fa96a4bbcfc370b63cd7e8955aab0e6793cd90f4227373a68ecc91b"
        }
    },
    "installer": {
        "script": [
            "$file = (Get-ChildItem \"$dir\" 'x64' | Select-Object -First 1).FullName",
            "Expand-7ZipArchive \"$file\" \"$dir\\\" -Removal",
            "Expand-7ZipArchive \"$dir\\`$PLUGINSDIR\\app-64.7z\" \"$dir\" -Removal",
            "Remove-Item \"$dir\\`$PLUGINSDIR\",\"$dir\\`$R0\",\"$dir\\`$LOCALAPPDATA\"  -Force -Recurse"
        ]
    },
    "shortcuts": [
        [
            "Cursor.exe",
            "Cursor"
        ]
    ],
    "checkver": {
        "script": [
            ". \"$pwd\\bin\\utils.ps1\"",
            "$dl_info = get_installer_info 'Anysphere/Cursor'",
            "\"ver:$($dl_info.PackageVersion);\"",
            "\"x64:$($dl_info.x64_exe.InstallerUrl);\"",
            "\"hash:$($dl_info.x64_exe.InstallerSha256);\""
        ],
        "regex": "ver:(?<version>.+?); x64:(?<x64>.+?); hash:(?<hash>.+?);"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "$matchX64",
                "hash": "$matchHash"
            }
        }
    }
}